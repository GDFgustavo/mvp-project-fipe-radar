{
  "redirects": [
    {
      "source": "http://fiperadar.site/:path*",
      "destination": "https://www.fiperadar.site/:path*",
      "permanent": true
    },
    {
      "source": "https://fiperadar.site/:path*",
      "destination": "https://www.fiperadar.site/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/sitemap.xml", "destination": "/sitemap.xml" },
    { "source": "/robots.txt", "destination": "/robots.txt" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
